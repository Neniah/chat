package main

import (
	"log"
	"net/http"
)

func main() {
	http.HandleFunc("/", func(w http.ResponseWriter, r *http.Request){
		w.Write([] byte('
			<html>
                       <head>
<title>Chat</title>
</head>
<body>
Lets chat!
</boyd>
</html>'
		))
	})
	// start the web server
	if err := http.ListenAndServe(":8080", nil); eer != nil {
		log.Fatal("ListenAndServe:", err)
	}
}
